
<div class="col-md-4 col-md-offset-4">
<%= form_for :login do |f| %>
  <div>
      <%= f.label :email %> <br>
      <%= f.text_field :email %>
  </div>
  <div>
      <%= f.label :password %> <br>
      <%= f.password_field :password %>
  </div>
  <div class="submit">
      <p><%= f.submit 'Submit' %></p>
  </div>
<% end %>
</div>


