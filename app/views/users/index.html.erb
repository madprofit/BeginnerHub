<% if current_user %>
  <h1>Users</h1>
  <ul>
  <% @users.each do |user| %>
  <li><%= user.email %> - <%= user.password_digest %> - <%= user.name %> </li>
  <% end %>
  </ul>
  <form action="/logout" method="post">
    <input name="_method" value="DELETE" type="hidden">
    <button>Logout</button>
  </form>
<% else %>
  <h4 class=intro>Please log in</h4>
<% end %>
